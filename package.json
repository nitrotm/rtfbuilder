{
  "name": "rtfbuilder",
  "version": "0.9.8",
  "description": "A comprehensive JavaScript library for creating Rich Text Format (RTF) documents.",
  "keywords": [
    "rtf",
    "document",
    "generator",
    "writer",
    "creator"
  ],
  "author": {
    "name": "Antony Ducommun",
    "url": "https://github.com/nitrotm"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nitrotm/rtfbuilder.git"
  },
  "bugs": {
    "url": "https://github.com/nitrotm/rtfbuilder/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "packageManager": "yarn@4.9.1",
  "type": "module",
  "types": "dist/cjs/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json",
    "./html": {
      "import": {
        "types": "./dist/esm/html/index.d.ts",
        "default": "./dist/esm/html/index.js"
      },
      "require": {
        "types": "./dist/cjs/html/index.d.ts",
        "default": "./dist/cjs/html/index.js"
      }
    },
    "./ooxml": {
      "import": {
        "types": "./dist/esm/ooxml/index.d.ts",
        "default": "./dist/esm/ooxml/index.js"
      },
      "require": {
        "types": "./dist/cjs/ooxml/index.d.ts",
        "default": "./dist/cjs/ooxml/index.js"
      }
    },
    "./rtf": {
      "import": {
        "types": "./dist/esm/rtf/index.d.ts",
        "default": "./dist/esm/rtf/index.js"
      },
      "require": {
        "types": "./dist/cjs/rtf/index.d.ts",
        "default": "./dist/cjs/rtf/index.js"
      }
    },
    "./validation": {
      "import": {
        "types": "./dist/esm/validation/index.d.ts",
        "default": "./dist/esm/validation/index.js"
      },
      "require": {
        "types": "./dist/cjs/validation/index.d.ts",
        "default": "./dist/cjs/validation/index.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist/cjs/**/!(*.tsbuildinfo)",
    "dist/esm/**/!(*.tsbuildinfo)",
    "package.json",
    "LICENSE.txt",
    "README.md"
  ],
  "dependencies": {
    "fflate": "^0.8",
    "rtfbuilder-jsx-runtime": "^1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@xmldom/xmldom": "^0.8",
    "tslib": "^2",
    "tsx": "^4",
    "typescript": "^5"
  },
  "resolutions": {
    "rtfbuilder-jsx-runtime": "portal:./jsx-runtime"
  },
  "scripts": {
    "build": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json"
  }
}
